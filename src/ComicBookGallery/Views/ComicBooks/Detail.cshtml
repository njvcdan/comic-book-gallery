@{
    Layout = null;

    var seriesTitle = "Title";
    var issueNumber = 700;
    var description = "<p>Final issue!<strong>Will Peter?!?!?!</strong></p>";
    var artists = new String[]
    {
        "Script: Name Name",
        "Story: Story Name",
        "Edits: Edits Name"
    };
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>
        <h1>@seriesTitle</h1>
        <h2>Issue #@issueNumber</h2>

        <h5>Description:</h5>
        <div>@Html.Raw(description)</div>

        <h5>Artists:</h5>
        <div>@artists</div>
    </div>
</body>
</html>
